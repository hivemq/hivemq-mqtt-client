Automatic-Module-Name: ${def;bsn}
Bundle-Description: HiveMQ Java MQTT Client 
Bundle-License: Apache-2.0; \
  description="Apache License, Version 2.0"; \
  link="https://opensource.org/licenses/Apache-2.0"
Bundle-DocURL: https://hivemq.github.io/hivemq-mqtt-client/
Bundle-SCM: url=https://github.com/hivemq/hivemq-mqtt-client, \
  connection=scm:git:https://github.com/hivemq/hivemq-mqtt-client.git, \
  developerConnection=scm:https://github.com/hivemq/hivemq-mqtt-client.git, \
  tag=${Git-Descriptor}
Export-Package: \
  com.hivemq.client.annotations.*, \
  com.hivemq.client.mqtt.*, \
  com.hivemq.client.rx.*, \
  com.hivemq.client.util.*
-consumer-policy: ${range;[==,=+)}